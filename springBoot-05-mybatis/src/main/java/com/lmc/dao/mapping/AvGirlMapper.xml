<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lmc.dao.AvGirlMapper">
    <select id="selectAll" resultType="AvGirl">
        select * from avgirl
    </select>

    <select id="selectById" parameterType="int" resultType="AvGirl">
        select * from avgirl where id = #{id}
    </select>
</mapper>